<odoo>
  <data noupdate="1">
    <record id="ir_cron_fotoapp_asset_lifecycle" model="ir.cron">
      <field name="name">FotoApp - Ciclo de vida de fotos</field>
      <field name="model_id" ref="model_tienda_foto_asset"/>
      <field name="state">code</field>
      <field name="code">model.cron_manage_photo_lifecycle()</field>
      <field name="interval_number">1</field>
      <field name="interval_type">days</field>
      <field name="active">True</field>
      <field name="nextcall" eval="(DateTime.now() + relativedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
    </record>
  </data>
</odoo>